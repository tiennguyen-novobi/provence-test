<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="product_exported_field_tree_view" model="ir.ui.view">
        <field name="name">product.exported.field.tree</field>
        <field name="model">product.exported.field</field>
        <field name="priority" eval="10"/>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="level"/>
                <field name="platform"/>
            </tree>
        </field>
    </record>
    <record id="product_exported_field_form_view" model="ir.ui.view">
        <field name="name">product.exported.field.tree</field>
        <field name="model">product.exported.field</field>
        <field name="priority" eval="10"/>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="level"/>
                        <field name="platform"/>
                    </group>
                </sheet>
                
            </form>
        </field>
    </record>
</odoo>