<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_update_done_quantities" model="ir.ui.view">
        <field name="name">update.done.quantities.view.form</field>
        <field name="model">update.done.quantities</field>
        <field name="arch" type="xml">
            <form string="Auto update done quantities?">
                <group>
                    <p>
                        You have not recorded <i>done</i> quantities yet, by clicking on <i>apply</i>
                        System will process all the quantities.
                    </p>
                </group>
                <footer>
                    <button name="process" string="_Apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
