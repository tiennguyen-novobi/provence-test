<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Amazon orders -->
        <record id="seq_fbm_order" model="ir.sequence">
            <field name="name">FBM Orders</field>
            <field name="code">fbm.order</field>
            <field name="prefix">FBM</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_fba_order" model="ir.sequence">
            <field name="name">FBA Orders</field>
            <field name="code">fba.order</field>
            <field name="prefix">FBA</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
