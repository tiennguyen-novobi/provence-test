<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="auto_buy_label_log_view_tree" model="ir.ui.view">
        <field name="name">auto.buy.label.log.view.tree</field>
        <field name="model">omni.log</field>
        <field name="inherit_id" ref="omni_log.omni_log_base_export_tree_view"/>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <field name="operation_type" position="after">
                <field name="order_id" string="Order Number"/>
            </field>
        </field>
    </record>

    <record id="auto_buy_label_log_view_form" model="ir.ui.view">
        <field name="name">auto.buy.label.log.view.form</field>
        <field name="model">omni.log</field>
        <field name="inherit_id" ref="omni_log.omni_log_base_export_form_view"/>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <field name="operation_type" position="after">
                <field name="order_id" string="Order Number"/>
            </field>
        </field>
    </record>
</odoo>