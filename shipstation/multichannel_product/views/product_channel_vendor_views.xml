<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="product_channel_vendor_tree_view" model="ir.ui.view">
        <field name="name">product.channel.vendor.tree</field>
        <field name="model">product.channel.vendor</field>
        <field name="arch" type="xml">
            <tree string="Vendor" editable="top">
                <field name="name" string="Name" required="1"/>
            </tree>
        </field>
    </record>

    <record id="action_product_channel_vendor" model="ir.actions.act_window">
        <field name="name">Vendor</field>
        <field name="res_model">product.channel.vendor</field>
        <field name="view_mode">tree,form</field>
    </record>

